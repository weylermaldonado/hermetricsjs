{
  "name": "hermetrics",
  "version": "0.0.0-development",
  "description": "Javascript version of hermetrics.py",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/weylermaldonado/hermetricsjs.git"
  },
  "keywords": [
    "hermetrics",
    "nlp",
    "similarity metric",
    "distance metric",
    "levenshtein",
    "jaro",
    "jaro-winkler",
    "hamming",
    "osa",
    "damerau-levenshtein",
    "jaccard",
    "dice"
  ],
  "scripts": {
    "test": "npx mocha -r ts-node/register tests/**/**/*.test.ts",
    "dev": "npx tsc --watch",
    "start": "nodemon dist/levenshtein.js",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepublish": "npx tsc",
    "semantic-release": "semantic-release"
  },
  "author": " <wmaldonado@soldai.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "chai": "^4.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-with-typescript": "^12.0.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "mocha": "^10.2.0",
    "semantic-release": "^17.0.7",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test && npm run lint --fix"
    }
  }
}
